// Manifest file tells Chrome everything it needs to know to properly load up the extension
{  
    "manifest_version": 2,  

    "name": "Google Docs Button Click",  
    "description": "Google Docs Button Click Simulator",  
    "version": "1.0",    
    "permissions": ["tabs", "https://*.drive.google.com/*","*://*/*", "contextMenus"],

    "browser_action": {  
        "default_icon": "cursor.png",  
        "default_popup": "popup.html"
    },
	"background":{
	 "scripts":["eventPage.js"],
	 "persistent": false
	}
}